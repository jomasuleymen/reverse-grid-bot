  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js -d ./typeormconfig.ts",
    "migration:create": "bash node_modules/.bin/typeorm migration:create ",
    "postmigration:create": "mv *-*.ts src/infrastructure/migrations",
    "migration:generate": "npm run typeorm migration:generate ",
    "postmigration:generate": "mv *-*.ts src/infrastructure/migrations",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "schema:drop": "npm run typeorm schema:drop"
  },
  "dependencies": {
    "@axiomhq/pino": "^1.2.0",
